{
  "compilerOptions": {
    "lib": ["dom", "es2017"],
    "outDir": "dist",
    "target": "es6",
    "declaration": false,
    "module": "commonjs",
    "strictNullChecks": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "node_modules"
  ]
}
